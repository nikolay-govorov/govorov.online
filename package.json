{
  "name": "govorov.online",
  "version": "2.0.0",
  "homepage": "https://govorov.online",
  "license": "Unlicense",
  "author": {
    "name": "Nikolay Govorov",
    "url": "https://govorov.online",
    "email": "nikolay_govorov@bk.ru"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": "nikolay-govorov/govorov.online",
  "scripts": {
    "clean": "rm -rf .cache _site",
    "start": "eleventy --serve",
    "build": "eleventy && postcss src/styles/main.css -d ./_site/styles/"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "postcss": "^8.2.4",
    "postcss-assets": "^5.0.0",
    "postcss-cli": "^8.3.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not ie_mob 11",
    "not op_mini all",
    "not op_mob > 0",
    "not and_qq > 0",
    "not android > 0"
  ],
  "postcss": {
    "map": "inline",
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {
        "stage": 0
      },
      "postcss-assets": {
        "loadPaths": [
          "./src/images"
        ]
      }
    }
  }
}
