{
  "name": "govorov.online",
  "version": "2.0.0",
  "homepage": "https://govorov.online",
  "license": "Unlicense",
  "author": {
    "name": "Nikolay Govorov",
    "url": "https://govorov.online",
    "email": "nikolay_govorov@bk.ru"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": "nikolay-govorov/govorov.online",
  "scripts": {
    "clean": "rm -rf .cache public",
    "start": "NODE_ENV=development gatsby develop",
    "build": "NODE_ENV=production gatsby build",
    "deploy": "echo 'govorov.online' > ./public/CNAME && gh-pages -d public",
    "lint:js": "eslint src/ --ext .js --fix",
    "lint:css": "stylelint \"src/**/*.css\" --fix",
    "test": "yarn lint:js && yarn lint:css"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.48",
    "gatsby-plugin-feed": "^2.0.9",
    "gatsby-plugin-postcss": "^2.0.1",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-remark-copy-linked-files": "^2.0.6",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-remark": "^2.1.12",
    "postcss": "^7.0.17",
    "postcss-assets": "^5.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "universal-url": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.1.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "babel": {
    "presets": [
      "babel-preset-gatsby"
    ],
    "plugins": [
      "transform-react-remove-prop-types"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not ie_mob 11",
    "not op_mini all",
    "not op_mob > 0",
    "not and_qq > 0",
    "not android > 0"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "postcss": {
    "map": "inline",
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {
        "stage": 0
      },
      "postcss-assets": {
        "loadPaths": [
          "./src/assets"
        ]
      }
    }
  }
}
